Articles(
    id: UUID,
    title: STR,
    timestamp: DATE,
    url: STR,
    categories vector: VECTOR,
    text: STR,
    publisher: STR,

    PK -> id
)

Users(
    id: UUID,
    name: STR,
    mail: STR,
    birthdate: DATE,
    location: ENUM,
    gender: ENUM,
    preferences: LIST[categories],
    interests: VECTOR,
    hashed_password: STR,
    created_at: TIMESTAMP
    
    PK -> id
)

Likes(
    article_id: UUID,
    user_id: UUID,
    like: BOOL,

    PK -> article_id (Genreted_articles.id), user_id (users.id)
    FK -> Genreted_articles.article_id, users.user_id
)

Genreted_articles(
    id: UUID,
    title: STR,
    timestamp: DATE,
    categories vector: VECTOR,
    text: STR

    PK -> id
)

Generated_from_Source(
    source_id: UUID
    gen_id: UUID

    PK -> source_id (Articles.id), gen_id (Genreted_articles.id)
    FK -> source_id (Articles.id), gen_id (Genreted_articles.id)
)







