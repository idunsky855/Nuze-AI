[users]
*id: UUID
email: TEXT
hashed_password: TEXT
name: TEXT
preferences: VECTOR
created_at: TIMESTAMP

[articles]
*id: UUID
title: TEXT
content: TEXT
source_url: TEXT
publisher: TEXT
published_at: TIMESTAMP
language: TEXT
scraped_at: TIMESTAMP
category_scores: VECTOR
metadata: JSONB

[categories]
*id: SERIAL
name: TEXT

[article_reads]
*user_id: UUID
*article_id: UUID
read_at: TIMESTAMP

[synthesized_articles]
*id: UUID
title: TEXT
content: TEXT
generated_at: TIMESTAMP
generation_prompt: TEXT
notes: TEXT

[synthesized_sources]
*synthesized_id: UUID
*article_id: UUID

[daily_summaries]
*id: UUID
user_id: UUID
article_ids: UUID[]
summary_generated_at: TIMESTAMP

article_reads.user_id > users.id
article_reads.article_id > articles.id
synthesized_sources.synthesized_id > synthesized_articles.id
synthesized_sources.article_id > articles.id
daily_summaries.user_id > users.id








